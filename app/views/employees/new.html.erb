<%= form_for :employee , url: employees_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
   <p>
    <%= f.label :gender %><br>
    <%= f.text_field :gender %>
  </p>
    <p>
    <%= f.label :age %><br>
    <%= f.text_field :age %>
  </p>
  
  <p>
    <%= f.label :telephone %><br>
    <%= f.text_area :telephone %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
